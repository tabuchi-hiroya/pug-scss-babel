{
  "name": "pug-scss-babel",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "node frontnote.js",
    "browser": "browser-sync start --config bs-config.js",
    "scss-css": "node-sass src/css/style.scss dist/css/style.css -w --source-map true",
    "post-css": "postcss --use autoprefixer dist/css/style.css -d dist/css/ -w",
    "build:styleguide": "node frontnote.js",
    "watch:pug": "pug src -w -o dist",
    "watch:css": "run-p scss-css post-css",
    "watch:webpack": "webpack -w",
    "start": "run-p browser watch:*"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^6.7.2",
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-preset-es2017": "^6.22.0",
    "browser-sync": "^2.18.6",
    "frontnote": "^2.0.5",
    "node-sass": "^4.4.0",
    "npm-run-all": "^4.0.1",
    "path": "^0.12.7",
    "postcss-cli": "^2.6.0",
    "pug": "^0.1.0",
    "pug-cli": "0.0.0",
    "webpack": "^1.14.0"
  }
}
